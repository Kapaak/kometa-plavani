{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/components/*":["components/*"],
      "@/styles":["styles"],
      "@/public/*":["public/*"],
      "@/redux/*":["redux/*"],
      "@/utils/*":["utils/*"],
      "@/libs":["libs/index.ts"],
      "@/shared":["components/Shared"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/_document.jsx"],
  "exclude": ["node_modules"],
}
